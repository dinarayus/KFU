#include <iostream>
#include <fstream>
// Функция для подсчета суммы цифр числа
int digitSum(int number) {
    int sum = 0;
    while (number != 0) {
        sum += number % 10;
        number /= 10;
    }
    return sum;
}
void vybor(int a[], int N) {
	int count = 0;
	for (int i = 0; i < N - 1; i++) {
		int min = i;
		for (int j = i + 1; j < N; j++) {

			if (digitSum(a[j]) < digitSum(a[min]))
				min = j;
		}
		if (min != i) {
			int tmp = a[min];
			a[min] = a[i];
			a[i] = tmp;
		}
	}
 }
 
int main() {
    ifstream inputFile("input.txt"); // Открытие файла для чтения
    ofstream outputFile("output.txt"); // Открытие файла для записи
    int N; 
    inputFile >> N;

    int* numbers = new int[N];
    
    for (int i = 0; i < arraySize; ++i) {
        inputFile >> numbers[i];
    }


    for (int i = 0; i < N; ++i) {
        fout << vybor(numbers[i], N) << " ";
    }

    delete[] numbers;
    inputFile.close(); // Закрытие файла для чтения
    outputFile.close(); // Закрытие файла для записи

    return 0;
}
