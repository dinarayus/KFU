#include<iostream>
using namespace std;

void Zapolneniye_mass(int N) {
	int *array;
	array = new int[N];
	for (int i = 0; i < N; i++) {
		cin >> array[i];
	}
	for (int i = 0; i < N; i++) {
		cout << array[i] << " ";
	}
 }
void back_mass(int array[],int N) {
	for (int i = N - 1; i >= 0; i--)
	{

		cout << array[i] << " ";

	}
}
int main() {
	int num;
	cin >> num;
	Zapolneniye_mass(num)
	back_mass(num);

}
