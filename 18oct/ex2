#include<iostream>
#include<iostream>
using namespace std;


void back_mass(int array[], int N) {
	for (int i = N - 1; i >= 0; i--)
	{

		cout << array[i] << " ";

	}
}
int main() {
	int num;
	cin >> num;
	int *array;
	array = new int[num];
	for (int i = 0; i < num; i++) {
		cin >> array[i];
	}
	cout << endl;
	back_mass(array, num);

}
